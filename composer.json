{
  "name": "majidmvulle/vehicle-bundle",
  "license": "None",
  "type": "symfony-bundle",
  "description": "Library for managing vehicle makes, models and trims.",
  "authors": [
    {
      "name": "Majid Mvulle",
      "email": "majid@majidmvulle.com"
    }
  ],
  "autoload": {
    "psr-4": {"MajidMvulle\\Bundle\\VehicleBundle\\": "src/"}
  },
  "require": {
    "php": ">=7.1",
    "doctrine/doctrine-bundle": "^1.3",
    "doctrine/orm": "^2.6",
    "gedmo/doctrine-extensions": "^2.4",
    "jms/serializer": "^1.10.0",
    "majidmvulle/utility-bundle": "^0.1.9",
    "sensio/framework-extra-bundle": "^5.1",
    "sonata-project/admin-bundle": "^3.29.0",
    "symfony/form": "^3.0 || ^4.0",
    "symfony/twig-bundle": "^3.0 || ^4.0",
    "symfony/yaml": "^3.0 || ^4.0"
  },
  "require-dev": {
    "symfony/console": "^3.0 || ^4.0",
    "symfony/phpunit-bridge": "^3.0 || ^4.0"
  },
  "config": {
    "platform": {
      "php": "7.1.11"
    },
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@bitbucket.org:majidmvulle/vehicle-bundle.git"
    },
    {
      "type": "vcs",
      "url": "git@bitbucket.org:majidmvulle/utility-bundle.git"
    }
  ]
}
